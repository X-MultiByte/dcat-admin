{
    "name": "{package}",
    "description": "{description}",
    "type": "{type}",
    "keywords": [{keywords}],
    "homepage": "https://github.com/{package}",
    "license": "{license}",
    "authors": [
        {authors}
    ],
    "require": {
        "php": ">=7.1.0",
        "dcat/laravel-admin": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "{namespace}": "src/"
        }
    },
    "extra": {
            "dcat-admin": "{namespace}{className}ServiceProvider",
            "laravel": {
                "providers": [
                    "{namespace}{className}ServiceProvider"
                ]
            }
    }
}
